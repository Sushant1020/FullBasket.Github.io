<%layout('layouts/boilerplate')%>

<body>

  <form action="/login" method="POST" class="validated-form" novalidate>
    <div class="container" id="card1">
      <div class="container col-md-4 mb-3">
        <h6>Login Your Account</h6>
      </div>
      <div class="container col-md-4 mb-3">
        <label class="form-label" for="username">Username</label>
      </div>
      <div class="container col-md-4 mb-3">
        <input class="form-control" type="username" id="username" name="username" required>
        <div class="valid-feedback">
          Looks Good
        </div>
      </div>



      <div class="container col-md-4 mb-3">
        <label class="form-label" for="password">Password</label>
      </div>
      <div class="container col-md-4 mb-3">
        <input class="form-control" type="password" id="password" name="password" required>
        <div class="valid-feedback">
          Looks Good
        </div>
      </div>
      <div class="container col-md-4 mb-3">
        <button class="btn btn-success">Login</button>
      </div>
    </div>
    </div>
  </form>




  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
    crossorigin="anonymous"></script>

  <script src="/public/javascripts/validateForms.js"></script>

  <script src="https://cdn.jsdelivr.net/npm/bs-custom-file-input/dist/bs-custom-file-input.min.js"></script>

</body>